{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.10"
    },
    "require-dev": {
        "phploc/phploc": "^4.0",
        "phpstan/phpstan": "^0.11.1",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
    }
}
