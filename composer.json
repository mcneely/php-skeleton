{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-SimpleXML": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phploc/phploc": "^7.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
